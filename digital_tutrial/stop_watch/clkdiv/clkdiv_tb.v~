module clkdiv_tb;

// parameter define
parameter CYCLE 	= 6250000000;		//ps 16kHz
parameter HALF_CYCLE 	= 3125000000;		//ps 
parameter DELAY = 100;

reg	I_RSTN, I_CLK;
wire	O_EN_1MS;

clkdiv clkdiv(
	.I_CLK	(I_CLK),
	.I_RSTN	(I_RSTN),
	.O_EN_1MS (O_EN_1MS)
	);

always #(HALF_CYCLE) begin	
	I_CLK = ~I_CLK;
end

initial begin
	I_RSTN = 0;
	I_CLK=0;
	#100
	I_RSTN = 1;
	#(100*CYCLE) $finish;
end

endmodule